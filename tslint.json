{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "eofline": false,
    "no-trailing-whitespace": false,
    "comment-format": false,
    "quotemark": false,
    "no-console": false,
    "one-line": false,
    "no-consecutive-blank-lines": false,
    "curly": false,
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore"
    ]
  },
  "exclude": [
    "build/**/*",
    "node_modules/**/*"
  ],
  "rulesDirectory": []
}
