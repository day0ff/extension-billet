{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "eofline": false,
    "no-trailing-whitespace": false,
    "comment-format": false,
    "quotemark": false,
    "no-console": false,
    "one-line": false,
    "no-consecutive-blank-lines": false,
    "curly": false,
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore"
    ],
    "whitespace": [
      true,
      "check-branch",
      "check-operator",
      "check-typecast"
    ],
    "trailing-comma": [
      true,
      {
        "multiline": "never",
        "singleline": "never"
      }
    ]
  },
  "exclude": [
    "build/**/*",
    "prod",
    "node_modules/**/*"
  ],
  "rulesDirectory": []
}
